/ {
    aliases {
        rbdc-serial-port = &sixtron_connector_1_uart;
    };
};

&sixtron_connector_1_uart {
	current-speed = <115200>;
	status = "okay";
};

&uart7 {
	pinctrl-0 = <&uart7_tx_pe8 &uart7_rx_pe7>;
	pinctrl-names = "default";

	current-speed = <230400>;
	status = "okay";

  	lidar0: lidar_0 {
		compatible = "ldrobot,ld19";
	};
};
