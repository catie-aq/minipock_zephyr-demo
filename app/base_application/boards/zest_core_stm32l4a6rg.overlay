/ {
    aliases {
        uros-serial-port = &usart3;
        rbdc-serial-port = &usart2;
    };
};

&usart3 {
    tx-rx-swap;
    current-speed = <460800>;
    status = "okay";
};

&usart2 {
	pinctrl-0 = <&usart2_tx_pa2 &usart2_rx_pa3>;
	pinctrl-names = "default";
	current-speed = <115200>;
	status = "okay";
};
